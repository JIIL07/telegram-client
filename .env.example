# Telethon credentials (https://my.telegram.org)
API_ID=12345678
API_HASH=your_api_hash
SESSION_NAME=session

# PostgreSQL connection to DB with myaso.clients
POSTGRES_DSN=postgresql://user:pass@host:5432/dbname

# Main AI bot API URL
MYASO_API_URL=http://app:8000
MYASO_FORWARD_ENDPOINT=/get_message
MYASO_API_TIMEOUT_SECONDS=20

# Logging
LOG_LEVEL=INFO
